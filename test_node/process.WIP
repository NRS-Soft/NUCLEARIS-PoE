const Process = require('../classes/Process');

new process = new Process()

describe('Add Process', () => {
  it('Project expediente should be number', () => {
    expect(
      Block.addProcess(
        '41955',
        '0x939Ebe25C99B87a7c1284cDC45Fa7e865c0d73e0',
        'Mecanizado',
        'BGH'
      )
    ).rejects.toEqual(TypeError(`Expediente is not a number`));
  });
  it('Title of process should be string', () => {
    expect(
      Block.addProcess(
        41955,
        '0x939Ebe25C99B87a7c1284cDC45Fa7e865c0d73e0',
        456,
        'BGH'
      )
    ).rejects.toEqual(TypeError(`Title of Process is not a string`));
  });
  it('Name of supplier should be string', () => {
    expect(
      Block.addProcess(
        41955,
        '0x939Ebe25C99B87a7c1284cDC45Fa7e865c0d73e0',
        'Mecanizado',
        123
      )
    ).rejects.toEqual(TypeError(`Name of supplier is not a string`));
  });
});
