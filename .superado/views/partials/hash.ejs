<% include header %>

<div class="alert alert-success" role="alert">
	<i class="fas fa-file-upload"></i>
	<div style="margin-top:70px;">
		<p><b><%= texts.check.saved %></b><br>
			<strong><%= texts.check.saved_info %></strong>
		</p>
		<p>
			<a class="btn btn-info" data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false"
				aria-controls="collapseExample">
				<%= texts.check.detalles %>
			</a>
			<a class="btn btn-info" href="javascript:history.back()">Volver</a>

		</p>
		<div class="collapse" id="collapseExample">
			<div class="card card-body">
				<p><b><%= texts.check.fileid %></b><br />
					<%= docid %>
				</p>
				<p><b><%= texts.check.hash %></b><br />
					<%= hashed %>
				</p>
				<p><b><%= texts.check.txhash %></b><br />
					<%= result %>
				</p>
			</div>
		</div>
	</div>
</div>

<% include footer %>